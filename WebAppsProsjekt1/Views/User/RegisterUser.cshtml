@model WebAppsProsjekt1.Models.User
@{
    ViewBag.Title = "RegisterUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Register</h2>

<form action="/User/NewUser" method="post">
    <table>
        <tr>
            <td>
                Email
            </td>
            <td>
                <input type="text" name="Email" />
            </td>
            <td>
                Password
            </td>
            <td>
                <input type="text" name="Password" />
            </td>
        </tr>
    </table>
    <input type="submit" name="buttonSubmit" value="Register" />
</form>

