@model List<WebAppsProsjekt1.Models.Movie>
@{
    ViewBag.Title = "Liste";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<body>
    <div>
        <h2>Liste</h2>
        <table class="table">
            <tr>
                <th>Id</th>
                <th>Tittel</th>
                <th>Kategori</th>
                <th>Beløp</th>
            </tr>
            @foreach (var movie in Model)
            {
                <tr>
                    <td>@movie.Id</td>
                    <td>@movie.Title</td>
                    <td>@movie.Category</td>
                    <td>@movie.Value</td>
                </tr>
            }
        </table>
    </div>
</body>