@model List<WebAppsProsjekt1.Models.Movie>

@{
    ViewBag.Title = "ListMovie";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ListMovie</h2>

@foreach (var movie in Model)
{
<tr>
    <td>
        @movie.Title
    </td>
    <td>
        @movie.Category
    </td>
    <td>
        @movie.Cost
    </td>
    <td>
        <img src="@Url.Content(movie.ImagePath)" alt="image"/>
    </td>
</tr>
}

